{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/assets/*", "/api/*"]
  },
  "routes": [
    {
      "route": "/removal*",
      "rewrite": "/removal.html"
    },
    {
      "route": "/:serviceType*",
      "rewrite": "/removal.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
