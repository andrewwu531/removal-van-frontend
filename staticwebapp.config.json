{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/assets/*", "/api/*", "/removal"]
  },
  "routes": [
    {
      "route": "/removal",
      "rewrite": "/removal.html"
    },
    {
      "route": "/:serviceType",
      "rewrite": "/removal.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
